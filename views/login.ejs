<%- include('template/head') -%>
<div class="container text-center">
	<h1><%=title%></h1>
	<form id="form-login">
		<div class="form-group row">
			<label for="user" class="col-sm-2 col-form-label">usermail / username</label>
			<input type="text" id="user" class="col-sm-7 md-form mt-0" required>
		</div>
		<div class="form-group row">
			<label for="pwd" class="col-sm-2 col-form-label">password <i class="fa fa-eye see"></i></label>
			<input type="password" id="pwd" class="col-sm-7 md-form mt-0" required>
		</div>
		<input type="hidden" id="csrf" value="<%=token%>">
		<div class="form-group row">
			<button type="submit" class="btn btn-primary btn-md col-sm-2"><%=title%></button>
		</div>
	</form>
	<div class="form-group row">
    <div id="out" class="col-md-5 mb-2">
    </div>
  </div>
</div>
<%- include('template/foot') -%>